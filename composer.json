{
  "name": "kurozumi/line-notifier",
  "type": "symfony-notifier-bridge",
  "description": "Symfony Line Notifier Bridge",
  "keywords": [
    "line",
    "notifier"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "kurozumi",
      "email": "info@a-zumi.net"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "symfony/http-client": "^5.0|^6.0",
    "symfony/notifier": "^5.3|^6.0",
    "symfony/polyfill-mbstring": "^1.0"
  },
  "require-dev": {
    "symfony/event-dispatcher": "^5.0|^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Kurozumi\\Notifier\\Bridge\\Line\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
